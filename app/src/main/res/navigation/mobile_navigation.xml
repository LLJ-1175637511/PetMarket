<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_square">

    <fragment
        android:id="@+id/navigation_square"
        android:name="com.qyl.petmarket.ui.fragment.SquareFragment"
        android:label="@string/title_square"
        tools:layout="@layout/fragment_square" />

    <fragment
        android:id="@+id/navigation_main"
        android:name="com.qyl.petmarket.ui.fragment.MyPetFragment"
        android:label="@string/title_main"
        tools:layout="@layout/fragment_my_pet" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.qyl.petmarket.ui.fragment.UserMineFragment"
        android:label="@string/title_user"
        tools:layout="@layout/fragment_user" />
</navigation>